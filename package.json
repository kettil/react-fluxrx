{
  "name": "rxjs-playground",
  "version": "1.0.0",
  "description": "",
  "main": "app/index.js",
  "types": "app/index.d.ts",
  "scripts": {
    "test": "jest --config=jest.js",
    "test:watch": "jest --config=jest.js --watch",
    "test:ci": "jest --config=jest-ci.js",
    "start": "node app/index.js",
    "build": "npm run build:clean && npm run build:generate",
    "build:clean": "rm -rf ./app",
    "build:generate": "tsc",
    "prettier": "prettier --write './**/*.ts' './**/*.tsx' './**/*.js'",
    "coverage": "npm run coverage:clean && npm run coverage:generate",
    "coverage:clean": "rm -rf ./coverage",
    "coverage:generate": "jest --config=jest.js --coverage",
    "docs": "npm run docs:clean && npm run docs:generate",
    "docs:clean": "rm -rf ./docs",
    "docs:generate": "typedoc --out ./docs --mode modules './src'",
    "_preversion": "npm test",
    "_version": "npm run build",
    "_postversion": "git push && git push --tags && npm publish && npm run build:clean"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/create-subscription": "^16.4.1",
    "@types/hoist-non-react-statics": "^3.0.1",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.0.0",
    "@types/react": "^16.4.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-typescript": "^0.12.0",
    "jest": "^23.5.0",
    "prettier": "^1.12.1",
    "ts-jest": "^23.0.0",
    "typedoc": "^0.11.1",
    "typescript": "^3.0.0",
    "typescript-eslint-parser": "^15.0.0"
  },
  "dependencies": {
    "create-subscription": "^16.5.0",
    "react": "^16.5.0",
    "rxjs": "^6.3.2"
  }
}
